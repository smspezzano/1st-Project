<div id="signUP">


  <div class="row">
    <div class="col-xs-5 col-xs-offset-1 col-sm-6 col-sm-offset-1 
    col-md-11 col-md-offset-1 col-lg-10 col-lg-offset-2">
      <h1>Sign in</h1>
      <%= form_for(:session, url: sessions_path) do |f| %>

        <%= f.label :email %>
        <%= f.text_field :email %>

        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit "Sign in", class: "btn btn-large btn-success" %>
      <% end %>

    <p>New user? <%= link_to "Sign up now!", signup_path %></p>
    </div>
  </div>
</div>